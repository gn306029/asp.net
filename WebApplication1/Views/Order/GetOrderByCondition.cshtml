<table class="table table-striped">
    <tr><td id="OrderID">訂單編號</td><td id="CustomerName">客戶姓名</td><td id="EmployeeName">負責員工</td><td id="OrderDate">訂購日期</td><td id="RequireDate">需要日期</td><td id="ShipperDate">出貨日期</td><td></td><td></td></tr>
    @for (int i = 0; i < @ViewBag.Save.Count; i++)
    {

        <tr>
            <td>@ViewBag.Save[i].OrderID</td>
            <td>@ViewBag.Save[i].CustomerName</td>
            <td>@ViewBag.Save[i].EmployeeName</td>
            <td>@ViewBag.Save[i].OrderDate</td>
            <td>@ViewBag.Save[i].RequireDate</td>
            <td>@ViewBag.Save[i].ShippedDate</td>
            <td>
                @using (Html.BeginForm("DeleteOrder", "Order"))
                {
                    <input type="hidden" name="OrderID" value=@ViewBag.Save[i].OrderID />
                        <input type="submit" value="刪除" />
                }
            </td>
            <td>
                @using (Html.BeginForm("GetOrderDetail", "Order"))
                {
                    <input type="hidden" name="OrderID" value=@ViewBag.Save[i].OrderID />
                        <input type="submit" value="更新" />
                }
            </td>
        </tr>
    }
</table>

<script>
    $("#OrderID").click(function () {
